{
    "name": "rndemo",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "start": "react-native start",
        "test": "jest",
        "postinstall": "patch-package && scripts/postinstall.sh",
        "lint:fix": "eslint . --cache --fix",
        "format": "prettier . --write",
        "format:check": "prettier . --write '**/{app,test}/**/{*.js,*.ts,*.tsx,*.md,*.mdx} --ignore-path .prettierignore'",
        "prepare": "husky install",
        "pre-commit": "node scripts/commitFileCountCheck.mjs && node scripts/branchNameConvention.mjs"
    },
    "lint-staged": {
        "**/*": [
            "npm run lint:fix",
            "prettier --write --ignore-unknown"
        ]
    },
    "dependencies": {
        "@google/generative-ai": "^0.11.3",
        "@react-native-async-storage/async-storage": "^1.22.3",
        "@reduxjs/toolkit": "^2.2.1",
        "eslint-config-standard-with-typescript": "^43.0.0",
        "react": "18.2.0",
        "react-native": "0.73.1",
        "react-native-fs": "^2.20.0",
        "react-native-image-picker": "^7.1.2",
        "react-native-mmkv": "^2.12.2",
        "react-native-navigation": "^7.37.2",
        "react-native-paper": "^5.12.3",
        "react-native-safe-area-context": "^4.10.1",
        "react-native-snackbar": "^2.6.2",
        "react-native-static-safe-area-insets": "^2.2.0",
        "react-redux": "^9.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@babel/preset-env": "^7.20.0",
        "@babel/runtime": "^7.20.0",
        "@commitlint/cli": "^18.4.4",
        "@commitlint/config-conventional": "^18.4.4",
        "@react-native/babel-preset": "^0.73.18",
        "@react-native/eslint-config": "^0.73.2",
        "@react-native/metro-config": "^0.73.2",
        "@react-native/typescript-config": "^0.73.1",
        "@types/react": "^18.2.6",
        "@types/react-test-renderer": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^6.18.1",
        "@typescript-eslint/parser": "^6.18.1",
        "babel-jest": "^29.6.3",
        "eslint": "^8.56.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jest": "^27.6.1",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-prettier": "^5.1.2",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-native": "^4.1.0",
        "husky": "^8.0.0",
        "jest": "^29.6.3",
        "lint-staged": "^15.2.0",
        "patch-package": "^8.0.0",
        "postinstall-postinstall": "^2.1.0",
        "prettier": "2.8.8",
        "react-test-renderer": "18.2.0",
        "typescript": "5.0.4"
    },
    "engines": {
        "node": ">=18"
    }
}
